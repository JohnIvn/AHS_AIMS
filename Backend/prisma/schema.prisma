generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model staff_account {
  staff_id       String   @id @default(uuid())
  first_name     String
  last_name      String
  contact_number String?
  email          String   @unique
  password       String
  date_created   DateTime @default(now())
  status         String   @default("active")
}

model appoinment_details {
  user_id        String   @id @default(uuid())
  first_name     String
  last_name      String
  contact_number String?
  email          String   @unique
  date_created   DateTime @default(now())
  status         String   @default("active")
}
